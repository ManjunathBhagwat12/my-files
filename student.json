{
  "@context": {
    "@version": 1.1,
    "@protected": true,

    "type": "@type",
    "schema": "https://schema.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "StudentCredential": {
      "@id": "https://manjunathbhagwat12.github.io/my-files/student.json#StudentCredential"
    },

    "uuid": "schema:identifier",
    "email": "schema:email",
    "cFullname": "schema:name",
    "dateRegistered": {
      "@id": "schema:dateCreated",
      "@type": "xsd:dateTime"
    },

    "courses": {
      "@id": "schema:hasCourse",
      "@container": "@list"
    },

    "learningPartner": "schema:provider",
    "courseName": "schema:name",
    "courseHours": {
      "@id": "schema:timeRequired",
      "@type": "xsd:duration"
    },
    "skillLevel": "schema:educationalLevel",
    "courseProgress": {
      "@id": "schema:percentComplete",
      "@type": "xsd:decimal"
    },
    "courseGrade": {
      "@id": "schema:ratingValue",
      "@type": "xsd:string"
    },
    "status": "schema:courseMode",
    "completionDate": {
      "@id": "schema:dateCompleted",
      "@type": "xsd:date"
    },
    "certificateExpirationDate": {
      "@id": "schema:validThrough",
      "@type": "xsd:date"
    }
  }
}
